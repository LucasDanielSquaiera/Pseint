Proceso Cociente_Resto
	ESCRIBIR "Por favor ingrese dos números";
	Definir numero1, numero2, resto, cociente Como Entero;
	Leer numero1, numero2; 
	resto <- numero1 MOD	numero2;
	cociente <- numero1 / numero2;
	Escribir "El resto de la división correspondiente es: ", resto;
	Escribir "El cociente de la división correspondiente es: ", cociente;
FinProceso
